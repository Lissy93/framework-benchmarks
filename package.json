{
  "name": "weather-front",
  "version": "1.0.0",
  "description": "Cross-framework weather app comparison project",
  "scripts": {
    "help": "node scripts",
    "check": "node scripts/verify/check.js",
    "test": "node scripts/verify/test.js",
    "lint": "node scripts/verify/lint.js",
    "// RUN TESTS": "---------------------------------------------------------",
    "// START DEV SERVER": "---------------------------------------------------",
    "// LINT APPLICATIONS": "-------------------------------------------------",
    "// MISCELLANEOUS SETUP SCRIPTS": "---------------------------------------",
    "setup": "node scripts/setup",
    "install:playwright": "playwright install",
    "// PERFORMANCE TESTING": "--------------------------------------------",
    "perf": "node scripts/performance/index.js",
    "perf:lighthouse": "node scripts/performance/index.js --only lighthouse",
    "perf:bundle": "node scripts/performance/index.js --only bundle",
    "perf:runtime": "node scripts/performance/index.js --only runtime",
    "perf:vanjs": "node scripts/performance/index.js --frameworks vanjs",
    "perf:html": "node scripts/performance/index.js --html",
    "// PRODUCTION SERVER": "--------------------------------------------",
    "serve:single": "node scripts/performance/serve-single.js",
    "verify": "node scripts/verify/index.js",
    "dev:vanilla": "cd apps/vanilla && python3 -m http.server 3000 || python -m http.server 3000",
    "build:vanilla": "cd apps/vanilla && echo 'No build step required'",
    "test:vanilla": "timeout 120s npx playwright test --config=tests/config/playwright-vanilla.config.js --reporter=list",
    "dev:react": "cd apps/react && vite",
    "build:react": "cd apps/react && vite build",
    "test:react": "timeout 120s npx playwright test --config=tests/config/playwright-react.config.js --reporter=list",
    "dev:angular": "cd apps/angular && ng serve --port 3000",
    "build:angular": "cd apps/angular && ng build",
    "test:angular": "timeout 120s npx playwright test --config=tests/config/playwright-angular.config.js --reporter=list",
    "dev:svelte": "cd apps/svelte && vite dev --port 3000",
    "build:svelte": "cd apps/svelte && vite build",
    "test:svelte": "timeout 120s npx playwright test --config=tests/config/playwright-svelte.config.js --reporter=list",
    "dev:preact": "cd apps/preact && vite",
    "build:preact": "cd apps/preact && vite build",
    "test:preact": "timeout 120s npx playwright test --config=tests/config/playwright-preact.config.js --reporter=list",
    "dev:solid": "cd apps/solid && vite",
    "build:solid": "cd apps/solid && vite build",
    "test:solid": "timeout 120s npx playwright test --config=tests/config/playwright-solid.config.js --reporter=list",
    "dev:qwik": "cd apps/qwik && vite --port 3000",
    "build:qwik": "cd apps/qwik && vite build",
    "test:qwik": "timeout 120s npx playwright test --config=tests/config/playwright-qwik.config.js --reporter=list",
    "dev:vue": "cd apps/vue && vite --port 3000",
    "build:vue": "cd apps/vue && vite build",
    "test:vue": "timeout 120s npx playwright test --config=tests/config/playwright-vue.config.js --reporter=list",
    "dev:jquery": "cd apps/jquery && vite --port 3000",
    "build:jquery": "cd apps/jquery && vite build",
    "test:jquery": "timeout 120s npx playwright test --config=tests/config/playwright-jquery.config.js --reporter=list",
    "dev:alpine": "cd apps/alpine && python3 -m http.server 3000 || python -m http.server 3000",
    "build:alpine": "cd apps/alpine && echo 'No build step required'",
    "test:alpine": "timeout 120s npx playwright test --config=tests/config/playwright-alpine.config.js --reporter=list",
    "dev:lit": "cd apps/lit && vite --port 3000",
    "build:lit": "cd apps/lit && vite build",
    "test:lit": "timeout 120s npx playwright test --config=tests/config/playwright-lit.config.js --reporter=list",
    "dev:vanjs": "cd apps/vanjs && vite --port 3000",
    "build:vanjs": "cd apps/vanjs && vite build",
    "test:vanjs": "timeout 120s npx playwright test --config=tests/config/playwright-vanjs.config.js --reporter=list",
    "dev:all": "npm run dev:vanilla && npm run dev:react && npm run dev:angular && npm run dev:svelte && npm run dev:preact && npm run dev:solid && npm run dev:qwik && npm run dev:vue && npm run dev:jquery && npm run dev:alpine && npm run dev:lit && npm run dev:vanjs",
    "build:all": "npm run build:vanilla && npm run build:react && npm run build:angular && npm run build:svelte && npm run build:preact && npm run build:solid && npm run build:qwik && npm run build:vue && npm run build:jquery && npm run build:alpine && npm run build:lit && npm run build:vanjs",
    "test:all": "npm run test:vanilla && npm run test:react && npm run test:angular && npm run test:svelte && npm run test:preact && npm run test:solid && npm run test:qwik && npm run test:vue && npm run test:jquery && npm run test:alpine && npm run test:lit && npm run test:vanjs",
    "lint:all": "",
    "sync-assets": "cd scripts && python setup/sync_assets.py",
    "generate-mocks": "cd scripts && python setup/generate_mocks.py",
    "generate-scripts": "cd scripts && python setup/generate_scripts.py",
    "setup:all": "cd scripts && python setup/main.py",
    "serve:production": "node scripts/performance/production-server.js"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.40.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "chrome-launcher": "^1.2.0",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-vue": "^10.4.0",
    "express": "^4.21.2",
    "gzip-size": "^7.0.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1"
  },
  "keywords": [
    "weather",
    "frontend",
    "comparison",
    "vanilla-js",
    "react",
    "vue",
    "svelte",
    "angular"
  ],
  "author": "Alicia Sykes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lissy93/weather-front"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
