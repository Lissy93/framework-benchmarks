{
  "name": "weather-front",
  "version": "1.0.0",
  "description": "Cross-framework weather app comparison project",
  "scripts": {
    "help": "node scripts",
    "check": "node scripts/verify/check.js",
    "test": "node scripts/verify/test.js",
    "lint": "node scripts/verify/lint.js",

    "// RUN TESTS": "---------------------------------------------------------",
    "test:vanilla": "playwright test --config=tests/config/playwright-vanilla.config.js",
    "test:react": "playwright test --config=tests/config/playwright-react.config.js",
    "test:angular": "playwright test --config=tests/config/playwright-angular.config.js",
    "test:svelte": "playwright test --config=tests/config/playwright-svelte.config.js",
    "test:preact": "playwright test --config=tests/config/playwright-preact.config.js",
    "test:solid": "playwright test --config=tests/config/playwright-solid.config.js",
    "test:qwik": "playwright test --config=tests/config/playwright-qwik.config.js",
    "test:vue": "playwright test --config=tests/config/playwright-vue.config.js",
    "test:jquery": "playwright test --config=tests/config/playwright-jquery.config.js",

    "// START DEV SERVER": "---------------------------------------------------",
    "dev:vanilla": "cd apps/vanilla && npm run dev",
    "dev:react": "cd apps/react && npm run dev",
    "dev:angular": "cd apps/angular && npm run dev",
    "dev:svelte": "cd apps/svelte && npm run dev",
    "dev:preact": "cd apps/preact && npm run dev",
    "dev:solid": "cd apps/solid && npm run dev",
    "dev:qwik": "cd apps/qwik && npm run dev",
    "dev:vue": "cd apps/vue && npm run dev",
    "dev:jquery": "cd apps/jquery && npm run dev",

    "// LINT APPLICATIONS": "-------------------------------------------------",
    "lint:vanilla": "eslint 'apps/vanilla/**/*.js'",
    "lint:react": "eslint 'apps/react/**/*.{js,jsx}'",
    "lint:angular": "eslint 'apps/angular/**/*.{ts,html}'",
    "lint:svelte": "eslint 'apps/svelte/**/*.{js,svelte}'",
    "lint:preact": "eslint 'apps/preact/**/*.{js,jsx}'",
    "lint:solid": "eslint 'apps/solid/**/*.{js,jsx}'",
    "lint:qwik": "eslint 'apps/qwik/**/*.{ts,tsx}'",
    "lint:vue": "eslint 'apps/vue/**/*.{js,vue}'",
    "lint:jquery": "eslint 'apps/jquery/**/*.js'",

    "// MISCELLANEOUS SETUP SCRIPTS": "---------------------------------------",
    "setup": "node scripts/setup",
    "install:playwright": "playwright install",
    "sync-assets": "node scripts/setup/sync-assets.js",
    "generate-mocks": "node scripts/setup/generate-mocks.js",
    "generate-scripts": "node scripts/setup/generate-scripts.js"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.40.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0"
  },
  "keywords": [
    "weather",
    "frontend",
    "comparison",
    "vanilla-js",
    "react",
    "vue",
    "svelte",
    "angular"
  ],
  "author": "Alicia Sykes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lissy93/weather-front"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
