{
  "name": "weather-front",
  "version": "1.0.0",
  "description": "Cross-framework weather app comparison project",
  "scripts": {
    "test": "(npm run test:vanilla || echo 'Vanilla JS tests completed'); (npm run test:react || echo 'React tests completed'); (npm run test:angular || echo 'Angular tests completed'); (npm run test:svelte || echo 'Svelte tests completed'); (npm run test:preact || echo 'Preact tests completed'); (npm run test:solid || echo 'Solid.js tests completed'); (npm run test:qwik || echo 'Qwik tests completed'); echo 'All test suites completed'",
    "test:vanilla": "playwright test --config=tests/config/playwright-vanilla.config.js",
    "test:react": "playwright test --config=tests/config/playwright-react.config.js",
    "test:angular": "playwright test --config=tests/config/playwright-angular.config.js",
    "test:svelte": "playwright test --config=tests/config/playwright-svelte.config.js",
    "test:preact": "playwright test --config=tests/config/playwright-preact.config.js",
    "test:solid": "playwright test --config=tests/config/playwright-solid.config.js",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:unit": "node tests/unit/run-unit-tests.js",
    "test:all": "npm run test:unit && npm run test",
    "test:ci": "playwright test --reporter=github",
    "dev:vanilla": "cd apps/vanilla && python3 -m http.server 3000 || python -m http.server 3000",
    "dev:react": "cd apps/react && npm run dev",
    "dev:angular": "cd apps/angular && npm run dev",
    "dev:svelte": "cd apps/svelte && npm run dev",
    "dev:preact": "cd apps/preact && npm run dev",
    "dev:solid": "cd apps/solid && npm run dev",
    "install:playwright": "playwright install",
    "sync-assets": "node scripts/sync-assets.js",
    "generate-mocks": "node scripts/generate-mocks.js",
    "generate-scripts": "node scripts/generate-scripts.js",
    "test:qwik": "playwright test --config=tests/config/playwright-qwik.config.js",
    "dev:qwik": "cd apps/qwik && npm run dev"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "jsdom": "^26.1.0"
  },
  "keywords": [
    "weather",
    "frontend",
    "comparison",
    "vanilla-js",
    "react",
    "vue",
    "svelte",
    "angular"
  ],
  "author": "Alicia Sykes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lissy93/weather-front"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
